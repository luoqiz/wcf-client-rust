{
  "productName": "wcf-client-rust",
  "version": "0.0.2",
  "identifier": "com.iamteer.wcf",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "wcf-client-rust",
        "width": 800,
        "height": 600,
        "closable": true,
        "maximizable": false,
        "minimizable": false,
        "resizable": false,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources":[
      "./src/wcferry/lib",
      "./icons"
    ]
  }
}
